package JavaLearnings;

public class pattern13 {

	public static void main(String[] args) {
/*		1111111
		1111122
		1111333
		1114444
		1155555
		1666666
		7777777
*/
		for(int i=1;i<=7;i++){
			for(int j=1; j<=7;j++){
				if((i+j)>=8)
					System.out.print(i+" ");
				else
					System.out.print("1 ");
			}
			System.out.println();
		}
	}

}
