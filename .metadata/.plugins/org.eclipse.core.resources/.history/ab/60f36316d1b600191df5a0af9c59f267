package JavaLearnings;

public class pattern11 {

	public static void main(String[] args) {
/*
		1234567
		  234567
		    34567
		      4567
		        567
		          67
		            7
		          67
		        567
		      4567
		    34567
		  234567
		1234567
*/
		for(int i=1;i<=7;i++){
			for(int j=1;j<=i-1;j++){
				System.out.print(" ");
			}
			for(int j=1;j<=i;j++){
				System.out.print(j+"");
			}
		}
	}

}
