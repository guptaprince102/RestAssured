package JavaLearnings;

public class pattern16 {

	public static void main(String[] args) {
/*		0000000 
		0100000 
		0020000
		0003000
		0000400
		0000050
		0000006
*/
		for(int i=1;i<=rows;i++){
			for(int j=1;j<=rows;j++){
				if(i==j && i<=2)
					System.out.print(i+" ");
				else
					System.out.print("0 ");
			}
		}
	}

}
