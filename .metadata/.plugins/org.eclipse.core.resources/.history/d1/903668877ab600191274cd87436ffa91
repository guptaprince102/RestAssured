package JavaLearnings;

public class pattern5 {

	public static void main(String[] args) {
	/*	7 6 5 4 3 2 1
		7 6 5 4 3 2 
		7 6 5 4 3 
		7 6 5 4
		7 6 5
		7 6
		7
	*/
		for(int i=7; i>=1;i--){
			for(int j=1;j<=i;j++){
				int k=i-j+1;
				if(k<7)
					System.out.print(k+" ");
				else
					System.out.print(k+7+" ");
			}
			System.out.println();
		}
	}

}
